@page "/"
@page "/{genderUrl}"
@inject IPersonService PersonService

<PageTitle>BlazorDate</PageTitle>


<PersonList />

@code{
    [Parameter]
    public string? GenderUrl { get; set; } = null;

    protected override async Task OnParametersSetAsync()
    {
        await PersonService.GetPeople(GenderUrl);
    }
}